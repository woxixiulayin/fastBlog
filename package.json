{
  "name": "fastBlog",
  "version": "0.0.1",
  "description": "a light blog use fastify",
  "main": "index",
  "author": "Jackson Liu (416883813@qq.com)",
  "license": "MIT",
  "scripts": {
    "dev": "pm2 delete fast-blog-dev && pm2 start --watch ./src -n fast-blog-dev --attach --interpreter ts-node src/server.ts",
    "first-start": "tsc ./src/server.ts && pm2 start -n fast-blog-pro dist/server.js",
    "start": "git pull origin master && tsc -p tsconfig.json && pm2 restart fast-blog-pro"
  },
  "dependencies": {
    "@types/debug": "^0.0.30",
    "@types/mongoose": "^4.7.24",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.1",
    "fastify": "0.33.0",
    "fastify-cookie": "^1.0.3",
    "fastify-formbody": "^1.0.1",
    "fastify-session": "^0.1.0",
    "fastify-static": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "marked": "^0.3.6",
    "moment": "^2.19.1",
    "mongoose": "^4.12.4",
    "pino": "^4.8.1",
    "reflect-metadata": "^0.1.10",
    "tsconfig-paths": "^2.3.0",
    "typescript": "^2.5.3"
  }
}
